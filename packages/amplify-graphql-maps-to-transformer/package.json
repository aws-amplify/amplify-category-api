{
  "name": "@aws-amplify/graphql-maps-to-transformer",
  "version": "3.0.0",
  "description": "Amplify GraphQL @mapsTo transformer",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-amplify/amplify-category-api.git",
    "directory": "packages/amplify-graphql-maps-to-transformer"
  },
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "amplify",
    "graphql",
    "cloudformation",
    "aws",
    "mapsto",
    "transformer"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc && mkdirp lib/assets && cd resources/mapping-lambda-function && bestzip --force node ../../lib/assets/mapping-lambda.zip index.js",
    "watch": "tsc -w",
    "clean": "rimraf ./lib tsconfig.tsbuildinfo",
    "test": "jest",
    "extract-api": "ts-node ../../scripts/extract-api.ts"
  },
  "dependencies": {
    "@aws-amplify/graphql-transformer-core": "2.0.0",
    "@aws-amplify/graphql-transformer-interfaces": "3.0.0",
    "graphql-mapping-template": "4.20.11",
    "graphql-transformer-common": "4.25.0"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.80.0",
    "constructs": "^10.0.5"
  },
  "devDependencies": {
    "@aws-amplify/graphql-index-transformer": "2.0.0",
    "@aws-amplify/graphql-model-transformer": "2.0.0",
    "@aws-amplify/graphql-relational-transformer": "2.0.0",
    "@aws-amplify/graphql-searchable-transformer": "2.0.0",
    "graphql": "^15.5.0",
    "mkdirp": "^0.5.5",
    "@aws-amplify/graphql-transformer-test-utils": "0.1.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(src/__tests__/.*\\.(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**",
      "!src/__tests__/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80
      }
    }
  }
}
