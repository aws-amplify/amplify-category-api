{
  "name": "@aws-amplify/graphql-schema-generator",
  "version": "0.11.2",
  "description": "Amplify GraphQL schema generator",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-amplify/amplify-category-api.git",
    "directory": "packages/amplify-graphql-schema-generator"
  },
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "graphql",
    "schema",
    "aws",
    "amplify",
    "generator",
    "transformer"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc && cd vpc-db-lambda && rm -rf node_modules && npm install && tsc && cp -r node_modules lib && cp -r src/certs lib/certs && cd lib && bestzip --force node ../../lib/rds-schema-inspector.zip ./*",
    "watch": "tsc -w",
    "clean": "rimraf ./lib && rimraf ./vpc-db-lambda/lib",
    "test": "jest",
    "extract-api": "ts-node ../../scripts/extract-api.ts"
  },
  "dependencies": {
    "@aws-amplify/graphql-transformer-core": "3.3.1",
    "@aws-amplify/graphql-transformer-interfaces": "4.2.0",
    "@aws-sdk/client-ec2": "3.624.0",
    "@aws-sdk/client-iam": "3.624.0",
    "@aws-sdk/client-lambda": "3.624.0",
    "@aws-sdk/client-rds": "3.624.0",
    "csv-parse": "^5.5.2",
    "fs-extra": "11.1.1",
    "graphql": "^15.5.0",
    "graphql-transformer-common": "5.1.1",
    "knex": "~2.4.0",
    "mysql2": "~3.9.7",
    "ora": "^4.0.3",
    "pg": "~8.11.3",
    "pluralize": "^8.0.0",
    "typescript": "^4.8.4"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/pluralize": "^0.0.29"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testEnvironmentOptions": {
      "url": "http://localhost"
    },
    "testRegex": "(src/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "modulePathIgnorePatterns": [
      "__utils__"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true,
    "coverageProvider": "v8",
    "coverageThreshold": {
      "global": {
        "branches": 76,
        "functions": 74,
        "lines": 83
      }
    },
    "coverageReporters": [
      "clover",
      "text"
    ],
    "moduleNameMapper": {
      "^csv-parse/sync": "<rootDir>/../../node_modules/csv-parse/dist/cjs/sync.cjs"
    },
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/__tests__/"
    ],
    "snapshotFormat": {
      "escapeString": true,
      "printBasicPrototype": true
    }
  }
}
