{
  "name": "@aws-amplify/graphql-construct-alpha",
  "version": "0.3.2",
  "description": "AppSync GraphQL Api Construct using Amplify GraphQL Transformer.",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-amplify/amplify-category-api.git",
    "directory": "packages/amplify-graphql-api-construct"
  },
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "scripts": {
    "build": "jsii",
    "watch": "jsii --watch",
    "package": "jsii-pacmak",
    "clean": "rimraf lib tsconfig.json tsconfig.tsbuildinfo node_modules",
    "test": "jest"
  },
  "keywords": [
    "graphql",
    "cdk",
    "aws",
    "awscdk",
    "amplify",
    "api",
    "appsync"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@aws-amplify/graphql-transformer": "0.0.6",
    "@aws-amplify/graphql-transformer-core": "1.3.7",
    "@aws-amplify/graphql-transformer-interfaces": "2.2.4"
  },
  "bundledDependencies": [
    "@aws-amplify/graphql-transformer",
    "@aws-amplify/graphql-transformer-core",
    "@aws-amplify/graphql-transformer-interfaces"
  ],
  "devDependencies": {
    "@types/fs-extra": "^8.0.1",
    "@types/node": "^12.12.6",
    "rimraf": "^3.0.0",
    "ts-jest": "^26.4.4",
    "jsii": "^5.1.5",
    "jsii-pacmak": "^1.84.0"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.80.0",
    "constructs": "^10.0.5"
  },
  "jest": {
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "((\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80
      }
    }
  },
  "stability": "experimental",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "java": {
        "package": "aws.amplify.graphqlapiconstruct",
        "maven": {
          "groupId": "aws.amplify",
          "artifactId": "graphql-api-construct",
          "versionSuffix": ".PREVIEW"
        }
      }
    },
    "versionFormat": "full",
    "tsc": {
      "rootDir": "src",
      "outDir": "lib"
    }
  }
}