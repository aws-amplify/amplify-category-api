{
  "name": "amplify-category-api-graphql-transformers-e2e-tests",
  "version": "8.4.7",
  "description": "End to end functional tests for appsync supported transformers.",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-amplify/amplify-category-api.git",
    "directory": "packages/graphql-transformers-e2e-tests"
  },
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "graphql",
    "appsync",
    "aws"
  ],
  "scripts": {
    "e2e": "jest",
    "build-tests": "yarn tsc --build tsconfig.tests.json"
  },
  "dependencies": {
    "@aws-amplify/graphql-auth-transformer": "2.1.7",
    "@aws-amplify/graphql-relational-transformer": "1.2.3",
    "axios": "^0.26.0",
    "cloudform-types": "^4.2.0",
    "graphql": "^15.5.0",
    "graphql-transformer-common": "4.24.6",
    "graphql-transformer-core": "8.1.3",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "@aws-amplify/core": "^2.1.0",
    "@aws-amplify/graphql-default-value-transformer": "1.1.7",
    "@aws-amplify/graphql-index-transformer": "1.2.3",
    "@aws-amplify/graphql-maps-to-transformer": "2.1.7",
    "@aws-amplify/graphql-model-transformer": "1.3.3",
    "@aws-amplify/graphql-transformer-core": "1.3.3",
    "@aws-amplify/graphql-transformer-interfaces": "2.2.2",
    "@types/node": "^12.12.6",
    "aws-amplify": "^4.2.8",
    "aws-appsync": "^4.1.1",
    "aws-sdk": "^2.1113.0",
    "execa": "^5.1.1",
    "fs-extra": "^8.1.0",
    "graphql-auth-transformer": "7.2.57",
    "graphql-connection-transformer": "5.2.56",
    "graphql-dynamodb-transformer": "7.2.56",
    "graphql-elasticsearch-transformer": "5.2.57",
    "graphql-function-transformer": "3.3.47",
    "graphql-http-transformer": "5.2.56",
    "graphql-key-transformer": "3.2.56",
    "graphql-predictions-transformer": "3.2.56",
    "graphql-tag": "^2.10.1",
    "graphql-versioned-transformer": "5.2.56",
    "isomorphic-fetch": "^3.0.0",
    "jest-junit": "^12.0.0",
    "node-fetch": "^2.6.7",
    "ws": "^7.5.7"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testEnvironment": "node",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/node_modules/**",
      "!src/__tests__/**",
      "!**/*.d.ts"
    ],
    "reporters": [
      "default",
      "jest-junit",
      [
        "amplify-category-api-e2e-core/failed-test-reporter",
        {
          "publicPath": "./amplify-e2e-reports",
          "reportPath": "./amplify-e2e-reports/amplify-e2e-failed-test.txt"
        }
      ]
    ],
    "testURL": "http://localhost",
    "testRegex": "(src/__tests__/.*.test.*)$",
    "testPathIgnorePatterns": [
      "/lib/",
      "/resources/",
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "window": {},
      "ts-jest": {
        "tsconfig": "<rootDir>/tsconfig.tests.json"
      }
    }
  },
  "jest-junit": {
    "outputDirectory": "reports/junit/",
    "outputName": "js-test-results.xml",
    "usePathForSuiteName": "true",
    "addFileAttribute": "true"
  }
}
