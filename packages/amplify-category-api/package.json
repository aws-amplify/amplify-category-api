    "@aws-amplify/graphql-predictions-transformer": "1.2.0",
    "@aws-amplify/graphql-relational-transformer": "1.1.2",
    "@aws-amplify/graphql-searchable-transformer": "1.2.0",
    "@aws-amplify/graphql-transformer-core": "1.2.0",
