{
  "name": "@aws-amplify/graphql-transformer",
  "version": "2.1.1-ai.4",
  "description": "Amplify GraphQL Transformer Root Package",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-amplify/amplify-category-api.git",
    "directory": "packages/amplify-graphql-transformer"
  },
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rimraf lib tsconfig.tsbuildinfo node_modules",
    "test": "jest",
    "extract-api": "ts-node ../../scripts/extract-api.ts"
  },
  "keywords": [
    "graphql",
    "cloudformation",
    "aws",
    "amplify",
    "api",
    "transformer"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@aws-amplify/graphql-auth-transformer": "4.1.2-ai.1",
    "@aws-amplify/graphql-conversation-transformer": "0.2.1-ai.4",
    "@aws-amplify/graphql-default-value-transformer": "3.0.4-ai.1",
    "@aws-amplify/graphql-function-transformer": "3.1.1-ai.1",
    "@aws-amplify/graphql-generation-transformer": "0.2.2-ai.1",
    "@aws-amplify/graphql-http-transformer": "3.0.4-ai.1",
    "@aws-amplify/graphql-index-transformer": "3.0.4-ai.1",
    "@aws-amplify/graphql-maps-to-transformer": "4.0.4-ai.1",
    "@aws-amplify/graphql-model-transformer": "3.0.4-ai.1",
    "@aws-amplify/graphql-predictions-transformer": "3.0.4-ai.1",
    "@aws-amplify/graphql-relational-transformer": "3.0.4-ai.1",
    "@aws-amplify/graphql-searchable-transformer": "3.0.4-ai.1",
    "@aws-amplify/graphql-sql-transformer": "0.4.4-ai.1",
    "@aws-amplify/graphql-transformer-core": "3.1.2-ai.1",
    "@aws-amplify/graphql-transformer-interfaces": "4.1.1-ai.0"
  },
  "devDependencies": {
    "@aws-amplify/graphql-transformer-test-utils": "1.0.3-ai.1",
    "@types/node": "^18.0.0",
    "fs-extra": "^8.1.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^29.0.0"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.152.0",
    "constructs": "^10.3.0"
  },
  "jest": {
    "testEnvironmentOptions": {
      "url": "http://localhost"
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "((\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true,
    "coverageProvider": "v8",
    "coverageThreshold": {
      "global": {
        "branches": 59,
        "functions": 54,
        "lines": 74
      }
    },
    "coverageReporters": [
      "clover",
      "text"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/__tests__/"
    ],
    "snapshotFormat": {
      "escapeString": true,
      "printBasicPrototype": true
    }
  }
}
